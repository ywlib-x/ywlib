_ywm_ns_start___________________________________________________(yw)

using cat0 = char;
using cat1 = char8_t;
using cat2 = char16_t;
using cat4 = char32_t;

using int1 = signed char;
using int2 = signed short;
using int4 = signed int;
using int8 = signed long long;

using nat1 = unsigned char;
using nat2 = unsigned short;
using nat4 = unsigned int;
using nat8 = unsigned long long;

using fat4 = float;
using fat8 = double;

using poid = void*;
using cpoid = const void*;

enum class byte : nat1 {};

using catt = cat1;

_ywm_ns_start___________________________________________________(_std)
using wc_t = wchar_t;
using sl_t = signed long;
using ul_t = unsigned long;
using ld_t = long double;
